<% for user in @users: %>
  <div class="active-user">
    <a href="#/users/<%= user.name %>" class="user">
      <img src="//zooniverse-avatars.s3.amazonaws.com/ouroboros/<%= user.zooniverse_id %>" class="avatar" onerror="window.defaultAvatar(this)" />
      <%= user.name %>
    </a>
  </div>
<% end %>
