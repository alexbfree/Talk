<!DOCTYPE html>
<html>
<head>
  <title><%= t 'title' %></title>
  <%= stylesheet_link_tag "temp" %>
  <%= stylesheet_link_tag "communicator" %>

  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" type="text/javascript"></script>
  <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.3/jquery-ui.min.js" type="text/javascript"></script>
  <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.3/themes/base/jquery-ui.css" type="text/css" media="all" />
  <%= javascript_include_tag 'keyword_highlight/jquery.keywordHighlight.js'%>
  <%= javascript_include_tag 'application', 'rails' %>
  <%= csrf_meta_tag %>
</head>
<body>
	<%= render :partial=> "shared/top_bar.html.erb" %>
	<div class="wrapper">
		<div id="header">
			<div id="logo">				
				<%= link_to(image_tag('logo.png'), root_path) %>
			</div>
		</div>
		<div id="lcol">
			<%= render :partial=> "shared/sidebar" %>
		</div>
		<div id="rcol" class="ct">
		  <%= render :partial => "shared/flash", :locals => { :flash => flash } %>
			<%= yield %>
		</div>
		<div class="push"></div>
	</div>
</body>
</html>
