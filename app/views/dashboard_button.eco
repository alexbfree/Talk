<% user = require('zooniverse/lib/models/user').current %>
<% config = require('lib/config') %>

<% if user and config.dashboard: %>
  <div class="dashboard dashboard-link">
    <a target="_blank" href="http://tools.zooniverse.org/#/project/<%- config.project %>/<%- @zoo_ids.join(',') %>/Dashboard-from-<%- @name %>">Open in Dashboard</a>
  </div>
<% end %>
