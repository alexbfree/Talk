<% tags = @tags.map (tag) -> tag._id %>

<h2>Popular hashtags</h2>
<% if tags.length is 0: %>
  <div class="none">No hashtags</div>
<% else: %>
  <ul class="hashtag-links">
    <% for tag in tags: %>
      <!-- TO-DO: link to search href="#/hashtags/<%= tag %>" -->
      <li><a><%= tag %></a></li>
    <% end %>
  </ul>
<% end %>
